{
  "version": 2,
  "builds": [
    {
      "src": "cmd/main.go",
      "use": "@vercel/go",
      "build": "go build"
    }
  ],
  "routes": [
    {
      "src": "/previsoes\\?(.*)",
      "dest": "route.go"
    }
  ],
  "install": {
    "include": [
      "go.sum",
      "go.mod"
    ],
    "cmd": "go mod tidy"
  }
}

